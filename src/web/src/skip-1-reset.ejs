<!DOCTYPE html>
<html>
  <head>
    <% var data={title:'新密码'} %><% include ejs/head.ejs %>

    <script defer src="/assets/reset.js"></script>
  </head>

  <body>
    <div class="register_con">
      <div class="l_con fl">
        <a href="index.html" class="freg_logo"
          ><img src="images/assets/njcci.png" width="150px"
        /></a>
        <div class="reg_slogan"></div>
        <div class="reg_banner"></div>
      </div>

      <div class="fr_con fr">
        <div class="reg_title clearfix">
          <h2>新密码设置：</h2>
          <a href="login.html">登录</a>
        </div>
        <div class="reg_form clearfix">
          <form>
            <ul>
              <li>
                <label>密码:</label>
                <input type="password" name="pwd" id="pwd" />
                <span class="error_tip">提示信息</span>
              </li>
              <li>
                <label>确认密码:</label>
                <input type="password" name="cpwd" id="cpwd" />
                <span class="error_tip">提示信息</span>
              </li>
              <li class="find_sub">
                <input type="button" value="确 定" name="" id="reset_sub" />
              </li>
            </ul>
          </form>
        </div>
      </div>
    </div>
    <br />

    <% data={nomp:true} %><% include ejs/footer.ejs %>
  </body>
</html>
